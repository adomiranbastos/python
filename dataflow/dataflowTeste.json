{
	"name": "dataflowTeste",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDataset_ykt",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DestinationDataset_ykt",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          IDAtracacao as string,",
				"          CDTUP as string,",
				"          IDBerco as string,",
				"          {Berço} as string,",
				"          {Porto Atracação} as string,",
				"          {Apelido Instalação Portuária} as string,",
				"          {Complexo Portuário} as string,",
				"          {Tipo da Autoridade Portuária} as string,",
				"          {Data Atracação} as string,",
				"          {Data Chegada} as string,",
				"          {Data Desatracação} as string,",
				"          {Data Início Operação} as string,",
				"          {Data Término Operação} as string,",
				"          Ano as string,",
				"          Mes as string,",
				"          {Tipo de Operação} as string,",
				"          {Tipo de Navegação da Atracação} as string,",
				"          {Nacionalidade do Armador} as string,",
				"          FlagMCOperacaoAtracacao as string,",
				"          Terminal as string,",
				"          {Município} as string,",
				"          UF as string,",
				"          SGUF as string,",
				"          {Região Geográfica} as string,",
				"          {Nº da Capitania} as string,",
				"          {Nº do IMO} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          IDAtracacao as integer,",
				"          CDTUP as string,",
				"          IDBerco as string,",
				"          {Berço} as string,",
				"          {Porto_Atracação} as string,",
				"          {Apelido_Instalação_Portuária} as string,",
				"          {Complexo_Portuário} as string,",
				"          {Tipo_da_Autoridade_Portuária} as string,",
				"          {Data_Atracação} as timestamp,",
				"          Data_Chegada as timestamp,",
				"          {Data_Desatracação} as timestamp,",
				"          {Data_Início_Operação} as timestamp,",
				"          {Data_Término_Operação} as timestamp,",
				"          Ano as integer,",
				"          Mes as string,",
				"          {Tipo_de_Operação} as string,",
				"          {Tipo_de_Navegação_da_Atracação} as string,",
				"          Nacionalidade_do_Armador as integer,",
				"          FlagMCOperacaoAtracacao as integer,",
				"          Terminal as string,",
				"          {Município} as string,",
				"          UF as string,",
				"          SGUF as string,",
				"          {Região_Geográfica} as string,",
				"          {Nº_da_Capitania} as string,",
				"          {Nº_do_IMO} as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          CDTUP,",
				"          IDBerco,",
				"          {Berço},",
				"          Mes,",
				"          Terminal,",
				"          {Município},",
				"          UF,",
				"          SGUF",
				"     )) ~> sink1"
			]
		}
	}
}